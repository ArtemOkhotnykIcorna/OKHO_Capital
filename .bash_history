set -e

# --- helpers ---
mkunit() {
  local name="$1"
  local workdir="$2"
  local exec="$3"
  local mem="$4"
  local memswap="$5"
  local cpu="$6"

  cat > "/etc/systemd/system/${name}.service" <<EOF
[Unit]
Description=${name}
After=network-online.target
Wants=network-online.target

[Service]
Type=simple
WorkingDirectory=${workdir}
ExecStart=${exec}
Restart=always
RestartSec=5
Environment=PYTHONUNBUFFERED=1
MemoryMax=${mem}
MemorySwapMax=${memswap}
CPUQuota=${cpu}

[Install]
WantedBy=multi-user.target
EOF
}

# --- create units (bots: 220M, dashes: 140M) ---
mkunit "okho-prod-bot" "/root/binance" "/root/venv/bin/python3 /root/binance/bina.py" "220M" "400M" "80%"
mkunit "okho-prod-dash" "/root/binance" "/root/venv/bin/uvicorn dashprod:app --host 0.0.0.0 --port 9000 --workers 1" "140M" "300M" "60%"

